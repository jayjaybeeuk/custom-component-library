{
  "name": "@jayjaybeeuk/custom-component-library",
  "version": "independent",
  "description": "",
  "type": "module",
  "main": "index.js",
  "private": true,
  "workspaces": [
    "packages/atoms/*",
    "packages/molecules/*",
    "packages/organisms/*",
    "packages/templates/*",
    "packages/pages/*",
    "packages/types",
    "packages/theming"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "lint": "eslint 'packages/**/src/**/*.{ts,tsx}'",
    "lint:fix": "eslint 'packages/**/src/**/*.{ts,tsx}' --fix",
    "format": "prettier --write 'packages/**/src/**/*.{js,ts,tsx,json,css,scss,md}'",
    "build": "lerna run build",
    "build:storybook": "storybook build",
    "publish::packages": "lerna version --conventional-commits --yes && lerna publish from-package --yes",
    "publish::packages::local": "lerna publish --registry http://localhost:4873/",
    "publish::bump": "lerna publish --force-publish --canary --yes",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "storybook": "storybook dev -p 6006"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.7",
